    "require": {
    },
    "require-dev": {
        "eecli/eecli": "dev-master",
        "wikimedia/composer-merge-plugin": "dev-master",
        "phpspec/phpspec": "~2.1",
        "behat/behat": "~3.0",
        "behat/mink-extension": "~2.0",
        "behat/mink-selenium2-driver": "~1.2",
        "cevou/behat-screenshot-compare-extension": "~1.0@dev",
        "behat/mink-goutte-driver": "~1.1@dev"
    },
    "minimum-stability": "dev",
    "config": {
        "bin-dir": "bin"
    },
    "extra": {
        "merge-plugin": {
            "include": [
                "app/third_party/boardworld/composer.json"
            ]
        }
    }
